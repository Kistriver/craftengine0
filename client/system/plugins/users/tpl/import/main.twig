{%extends SYS.BASE~SYS.BASE_TPL%}

{%block title%}Импорт БД {{parent()}}{%endblock%}
{%block header%}
    <li><a href="{{MAIN.ROOT_HTTP}}admin">Админ панель</a></li>
    <li>Другое</li><li class="active">Импорт БД</li>
{%endblock%}

{%block content%}
    <form method="post">
    <div class="row">
        <div class="col-md-7 col-md-offset-1">
            <div class="input-group">
<span class="input-group-addon">
Движок
</span>
                <select name="engine" class="form-control">
                    <option disabled>ВАЗ-2105 — 1,3 л, карбюратор, 64 л.с.</option>
                    {% for value,name in supported %}
                        <option value="{{ value }}">{{ name }}</option>
                        {% endfor %}
                </select>
            </div>
        </div>
        <div class="col-md-3">
            <input type="submit" style="width: 100%;" class="btn btn-success" value="Импортировать" />
        </div>
        </div>
        </form>
{%endblock%}