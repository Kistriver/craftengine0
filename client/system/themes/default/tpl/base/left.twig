<nav class="navbar navbar-inverse navbar-static-top" role="navigation">

    <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="{{MAIN.ROOT_HTTP}}">{{MAIN.NAME}}</a>
    </div>

    <div class="collapse navbar-collapse navbar-ex1-collapse">
        <ul class="nav navbar-nav">
            {%if SYS.LOGGEDIN==false%}<li><a href="{{MAIN.ROOT_HTTP}}signup">Регистрация</a></li>{%endif%}
            <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown">Меню <b class="caret"></b></a>
                <ul class="dropdown-menu">
                    {%for menu in WIDGETS.menu%}
                        <li><a href="{{MAIN.ROOT_HTTP}}{{menu['1']}}">{{menu['0']}}</a></li>
                    {%endfor%}
                </ul>
            </li>
        </ul><!--
<form class="navbar-form navbar-left" role="search">
<div class="form-group">
<input type="text" class="form-control" placeholder="Поиск">
</div>
<button type="submit" class="btn btn-success">Поехали</button>
</form>-->
        <ul class="nav navbar-nav navbar-right">
            {%if SYS.LOGGEDIN==false%}
                <li class="dropdown">
                    <a href="{{MAIN.ROOT_HTTP}}login" class="dropdown-toggle" data-toggle="dropdown">Вход <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li>


                            <form method="post" action="{{MAIN.ROOT_HTTP}}login">

                                <div class="row">
                                    <div class="col-md-10 col-md-offset-1">
                                        <input name="email" maxlength="100" type="text" class="form-control" placeholder="Адрес электронной почты">
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-10 col-md-offset-1">
                                        <input name="password" maxlength="100" type="password" class="form-control" placeholder="Пароль">
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-10 col-md-offset-1">
                                        <input type="submit" style="width: 100%;" class="btn btn-success" value="Есть контакт!" />
                                    </div>
                                </div>
                            </form>




                        </li>
                    </ul>
                </li>
            {%else%}
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">{{SYS.USERS.LOGIN}} <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li><a href="{{MAIN.ROOT_HTTP}}profile/main">Настройки</a></li>
                        <li><a href="{{MAIN.ROOT_HTTP}}logout">Выход</a></li>
                    </ul>
                </li>
            {%endif%}
        </ul>
    </div>
</nav>

<!--CAROUSEL-->
<div style="margin: -21px 0px; padding: 0px;">
<center>
    <div id="myCarousel" class="carousel slide">
        <div class="carousel-inner" style="max-width: 1200px; margin: auto;">

            <div class="active item">
                <div class="carousel-caption" style="border-radius: 6px; background:url('{{MAIN.ROOT}}style/img/main/dark1.png');">
                    <h4>Сервера Minecraft</h4>
                    <p>Имеется несколько серверов Minecraft с <del>блэк джэком и ..</del> экономикой</p>
                </div>
                <img src="{{MAIN.ROOT}}style/img/index/3.png" style="height:275px;" />
            </div>

            <div class="item">
                <img src="{{MAIN.ROOT}}style/img/carousel/phplogo.png" style="height:275px" />
                <div class="carousel-caption" style="border-radius: 6px; background:url('{{MAIN.ROOT}}style/img/main/dark1.png');">
                    <h4>Собственный фреймворк</h4>
                    <p>Наш фреймворк легко расширяем и не имеет аналогов</p>
                </div>
            </div>

            <div class="item">
                <img src="{{MAIN.ROOT}}style/img/carousel/kcraftlogo.jpg" style="height:275px" />
                <div class="carousel-caption" style="border-radius: 6px; background:url('{{MAIN.ROOT}}style/img/main/dark1.png');">
                    <h4>Перспектива развития</h4>
                    <p>Наш проект радует Вас с 2011 года</p>
                </div>
            </div>

        </div>
        <a class="carousel-control left" href="#myCarousel" data-slide="prev"><span class="glyphicon glyphicon-chevron-left"></span></a>
        <a class="carousel-control right" href="#myCarousel" data-slide="next"><span class="glyphicon glyphicon-chevron-right"></span></a>
    </div>
</center>
</div>

<!--MAIN-->
<div style="margin: 0px 0px 0px 0px; padding: 30px 45px;">
    <div class="row" style="max-width: 1200px; margin: auto;">

        <div class="col-md-3" style="/*background: #F0F0FA;*/">
            {%for W in SYS.WIDGETS%}
                {%include SYS.WIDGET ~ W.0 ~ "/tpl/" ~ W.1 ~ ".twig"%}
            {%endfor%}
        </div>

        <div class="col-md-9">
            {% if SYS.NOHEADER==true %}{% else %}
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h1 class="panel-title">
                            <ol class="breadcrumb" style="padding: 0px; margin: 0px; background: none;">
                                {%block header%}<li>
                                    <a href="#root">Root</a>
                                </li>
                                    <li class="active">Untitled</li>
                                {%endblock%}
                            </ol>
                        </h1>
                    </div>
                {% if SYS.NOMAINBORDER==true %}</div>{% else %}<div class="panel-body">{% endif %}
            {% endif %}
            {% autoescape false %}
            {%for error in MAIN.ERRORS%}
                <div class="alert alert-danger">
                    {{error}}<br />
                </div>
            {%endfor%}
            {%for success in MAIN.SUCCESS%}
                <div class="alert alert-success">
                    {{success}}<br />
                </div>
            {%endfor%}
            {%for info in MAIN.INFO%}
                <div class="alert alert-info">
                    {{info}}<br />
                </div>
            {%endfor%}
            {% endautoescape %}
            {%block content%}
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h1 class="panel-title">
                            Title
                        </h1>
                    </div>
                    <div class="panel-body">
                        Main Page
                    </div>
                </div>
            {%endblock%}
            {% if SYS.NOMAINBORDER==true %}{% else %}</div>{% endif %}
            </div>
        </div>

    </div>
</div>

<div style="height: 300px;"></div>
